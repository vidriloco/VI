<!DOCTYPE html>
<html>
<head>
  <%= render_meta_data %>
  <%= stylesheet_link_tag 'application' %>
	<%= javascript_include_tag 'application' %>
	<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true"></script>
	
	<%= favicon_link_tag '/assets/favicon.ico' %>
	<%= favicon_link_tag '/assets/favicon-144.png', :size => '144x144', :rel => 'apple-touch-icon-precomposed', :type => 'image/png' %>
	<%= favicon_link_tag '/assets/favicon-72.png', :size => '72x72', :rel => 'apple-touch-icon-precomposed', :type => 'image/png' %>
	<%= favicon_link_tag '/assets/favicon.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png' %>
	<script type="text/javascript">

	  var _gaq = _gaq || [];
	  _gaq.push(['_setAccount', 'UA-47752323-1']);
	  _gaq.push(['_setDomainName', 'institute.vc']);
	  _gaq.push(['_trackPageview']);

	  (function() {
	    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
	    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
	    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
	  })();

	</script>

</head>
<body>
  <%= yield %>
  <%= alchemy_preview_mode_code %>
  <%= alchemy_menu_bar %>
</body>
</html>
