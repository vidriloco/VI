<%= element_editor_for(element) do |el| -%>
  <%= el.edit :content %>
	<%= el.edit :content_color %>
	<%= el.edit :content_margin_top %>
	<%= el.edit :content_floating_in %>
	<%= el.edit :content_justified_to %>
	<%= el.edit :button_visible %>
  <%= el.edit :button_link %>
  <%= el.edit :button_content %>
  <%= el.edit :image %>
<%- end -%>
