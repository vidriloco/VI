<%= element_editor_for(element) do |el| -%>
  <%= el.edit :name %>
  <%= el.edit :occupation, :select_values => ["Socio Director", "Socio Fundador", "Consultor", "Office Manager", "Program Manager"] %>
  <%= el.edit :linked_in %>
  <%= el.edit :twitter %>
  <%= el.edit :description %>
  <%= el.edit :image %>
<%- end -%>
